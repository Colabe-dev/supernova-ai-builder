# ---- Server base ----
PORT=3001
NODE_ENV=development

# ---- Security Pro ----
AUTH_JWKS_URL=http://localhost:3001/auth/.well-known/jwks.json
AUTH_ISSUER=https://collab.supernova.auth
AUTH_AUDIENCE=supernova-api
DEV_AUTH_OPEN=true

# ---- Redis ----
REDIS_URL=redis://localhost:6379
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_READ_MAX=120
RATE_LIMIT_WRITE_MAX=30
RATE_LIMIT_WEBHOOK_MAX=1200

# ---- Postgres ----
DATABASE_URL=postgres://supernova:supernova@localhost:5432/supernova

# ---- Observability ----
SENTRY_DSN=
SENTRY_ENV=dev

# ---- Collab Pay ----
COLLAB_PAY_API_BASE=http://localhost:4000
COLLAB_PAY_SECRET=dev
COLLAB_PAY_WEBHOOK_SECRET=

# ---- IAP ----
IAP_STRICT=false
DEV_IAP_OPEN=true
GOOGLE_SERVICE_ACCOUNT_KEY=
GOOGLE_PACKAGE_NAME=com.collab.supernova
APPLE_BUNDLE_ID=com.collab.supernova
IAP_USE_SANDBOX=true
APPLE_SHARED_SECRET=
